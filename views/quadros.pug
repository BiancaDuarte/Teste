doctype html
html
head
	title= title
	link(rel='stylesheet', href='/stylesheets/bootstrap.min.css')
	link(rel='stylesheet', href='/stylesheets/materialize.min.css')
	link(rel='stylesheet', href='/stylesheets/font-awesome.min.css')
	link(href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet")
	link(rel='stylesheet', href='/stylesheets/style.css')
	script(src='/../javascripts/jquery-3.1.1.min.js')
	script(src='/../javascripts/bootstrap.min.js')
	script(src='/../javascripts/materialize.min.js')
	script(src='/javascripts/jquery.zoom.min.js')
	script(src='/../javascripts/javascript.js')

body

	include logo 
	include body

	.container-fluid
		.row
			.col-md-2.col-sm-4.sidebar1
				.brand Filtrar busca:
					form(action='#')
								p
									input#test1(name='group1', type='radio')
									label(for='test1') Menor Preço
								p
									input#test2(name='group1', type='radio')
									label(for='test2') Maior Preço
				i.fa.fa-chevron-circle-down.fa-2x.toggle-btn(data-toggle='collapse', data-target='#menu-content')
				.menu-list
					ul#menu-content.menu-content.collapse.out
						
						li.collapsed.active(data-toggle='collapse', data-target='#filmes')
							a(href='#')
							i.fa.fa-film.fa-lg
							strong  Filmes
							span.arrow
						ul#filmes.sub-menu.collapse
							ul.list
									li(data-id="alice").filter Alice no País das Maravilhas
									li(data-id="bastardos").filter Bastardos Inglórios
									li(data-id="batman").filter Batman
									li(data-id="cisne").filter Cisne Negro
									li(data-id="clube").filter Clube da Luta
									li(data-id="donnie").filter Donnie Darko
									li(data-id="alice").filter Guardiões da Galáxia
									li(data-id="alice").filter Hobbit
									li(data-id="alice").filter Harry Potter
									li(data-id="alice").filter Interstellar
									li(data-id="alice").filter Kill Bill
									li(data-id="alice").filter Logan
									li(data-id="alice").filter Mad Max
									li(data-id="alice").filter Poderoso Chefão
									li(data-id="alice").filter Robocop
									li(data-id="alice").filter Senhor dos Anéis
									li(data-id="alice").filter Star Wars
						li.collapsed(data-toggle='collapse', data-target='#musica')
							a(href='#')
							i.fa.fa-music.fa-sm
							strong  Música
						ul#musica.sub-menu.collapse
								ul.list
									li#acdc AC/DC
									li Artic Monkeys
									li Black Sabbath
									li David Bowie
									li Led Zeppelin
									li Pink Floyd
									li The Beatles
									li Rolling Stones
									li Sex Pistols
						li.collapsed(data-toggle='collapse', data-target='#seriados')
							a(href='#')
							i.fa.fa-video-camera.fa-lg
							strong  Seriados
						ul#seriados.sub-menu.collapse
								ul.list
									li Breaking Bad
									li Dexter
									li Doctor Who
									li Game Of Thrones
									li How I Met You Mother
									li Stranger Things
									li Supernatural
						li.collapsed(data-toggle='collapse', data-target='#desenhos')
							a(href='#')
							i.fa.fa-child.fa-lg
							strong  Desenhos
						ul#desenhos.sub-menu.collapse
								ul.list
									li Aladdin
									li Bambi
									li Dragon Ball
									li Cisne Negro
									li Hora de Aventura
									li Toy Story

						
			.col-md-10
				div.titulopromo
					h2 Quadros
				div.branco
					div.container
						div.row
							#quadros
